{
    "env": {
        "browser": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "babel"
    ],
    "parser": "babel-eslint",
    "rules": {
        "no-invalid-this": 0,
        "babel/no-invalid-this": 1,
        "linebreak-style": 0,
        "react/jsx-props-no-spreading": 0,
        "react/forbid-prop-types": 0,
        "react/prop-types": 0,
        "no-underscore-dangle": 0,
        "no-unused-expressions" : 0
        
    }

}
